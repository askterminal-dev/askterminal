{
  "name": "ask-terminal",
  "version": "0.1.0",
  "description": "Beginner-friendly terminal with contextual explanations and safety indicators",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "node-pty": "^0.10.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.2",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.16",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.15.0",
    "vite-plugin-electron-renderer": "^0.14.0",
    "vue-tsc": "^1.8.0"
  },
  "build": {
    "appId": "dev.askterminal.app",
    "productName": "Ask Terminal",
    "files": [
      "dist/**/*",
      "!dist/mac-arm64/**",
      "!dist/*.dmg",
      "!dist/*.zip",
      "!dist/*.blockmap",
      "!dist/*.yaml",
      "dist-electron/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "build/icon.icns",
      "identity": "Greg Elin (29N25H78FA)",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "notarize": {
        "teamId": "29N25H78FA"
      }
    },
    "win": {
      "icon": "build/icon.icns"
    },
    "linux": {
      "icon": "build/icon.iconset"
    }
  }
}
